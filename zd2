#include <iostream>
#include <cmath>
using namespace std;
int i;
int main()
{
    for (int i = 0; i <= 100; i += 2) cout << i << ' ';

    int i = 0;
    while (i <= 100)
    {
        cout << ' ' << i;
        i += 2;
    }

    i = 0;
    do 
    {
        cout << ' ' << i;
        i += 2;

    } 
    while (i <= 100);

    return 0;
}
