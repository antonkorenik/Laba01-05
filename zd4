#include <iostream>
#include <cmath>
#include <cstdlib>
#include <ctime>
#define MAX_RAND_VALUE 10

using namespace std;
int i;

int main()
{
	srand(time(0));
	for (int i = 0; i <= 9; i++)
{
		cout << rand() % MAX_RAND_VALUE << " ";
}
	i = 0;
	while (i <= 9)
	{
		cout << rand() % 90+10 <<endl;
		i++;
	}
return 0;
}
 
